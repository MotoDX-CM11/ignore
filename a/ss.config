# Device specific
SYSTEM=mmcblk1p21
USERDATA=mmcblk1p24
CACHE=mmcblk1p22
BOOT=mmcblk1p15
SYSTEM_FSTYPE=auto
USERDATA_FSTYPE=auto
SS_FSTYPE=ext4
SS_PART=mmcblk1p24
SS_DIR=/ss/safestrap
HIJACK_BIN=e2fsck
HIJACK_LOC=bin
BOOTMODE_PROP=ro.bootmode
DEVICE_PROP=ro.product.device
CHARGER_MODE_SYSFS=/sys/class/power_supply/ac/online
CHECK_BOOTMODE=bp-tools
CHECK_POWERUP_REASON=/sbin/busybox cat /proc/bootinfo | /sbin/busybox grep POWERUPREASON | /sbin/bbx cut -c 19-
POWERUP_REASON_CHARGER=00000100
BACKLIGHT_BRIGHTNESS_PATH=/sys/class/leds/lcd-backlight/brightness
BACKLIGHT_BRIGHTNESS_VALUE=18
TASKSET_CPUS=0
SS_USE_DATAMEDIA=1
DEBUG_MODE=0

